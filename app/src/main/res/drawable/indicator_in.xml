    </item>
    <item android:width="6dp" android:height="3dp" android:top="3dp">
        <shape>
            <size android:width="4dp" android:height="3dp"/>
            <corners android:bottomLeftRadius="3dp" android:bottomRightRadius="3dp"/>
            <solid android:color="@color/prof"/>
        </shape>
    </item>

</layer-list>